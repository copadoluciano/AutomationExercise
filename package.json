{
  "name": "tutorial-js-cypress-cucumber",
  "version": "1.0.0",
  "description": "An example for Cypress and Cucumber usage using Robot login demo website",
  "main": "index.js",
  "scripts": {
    "cypress:open:local": "CYPRESS_ENV=localhost npm run cypress:open",
    "cypress:open:prod": "CYPRESS_ENV=production npm run cypress:open",
    "cypress:open": "npx cypress open",
    "test:local": "CYPRESS_ENV=localhost npm run test  --spec 'cypress/integration/**/*.feature",
    "test:prod": "CYPRESS_ENV=production npm run test",
    "test": "npx cypress run --record --key 024122da-9f30-4f9c-8f6e-43462be7c21b",
    "test:debug:local": "CYPRESS_ENV=localhost npm run test:debug",
    "test:debug:prod": "CYPRESS_ENV=production npm run test:debug",
    "test:debug": "npx cypress run --env TAGS='@forgot' --spec 'cypress/e2e/**/**/*.feature'",
    "report:formatter": "cat cucumber-json/cucumber-messages.ndjson | cucumber-json-formatter > cucumber-json/cucumber-report.json",
    "report:multiple": "node cucumber-html-report.js",
    "cypress:dashboard": "npx cypress run --record --key 024122da-9f30-4f9c-8f6e-43462be7c21b",
    "currents": "npx currents run --parallel --record --key zz0Uw7JjsYCFqLDm --ci-build-id build003",
    "counterparties": "npx cypress run -e TAGS='counterparties'",
    "forgotPassword": "npx cypress run -e TAGS='@TESTSET_LM-9074'",
    "home": "npx cypress run -e TAGS=\"@TESTSET_LM-9075\"",
    "loan": "npx cypress run -e TAGS=\"@TESTSET_LM-9075\"",
    "myBalancesAndWallets": "npx cypress run -e TAGS=\"@TESTSET_LM-9075\"",
    "securityCenter": "npx cypress run -e TAGS=\"@TESTSET_LM-9075\"",
    "signIn": "npx cypress run -e TAGS=\"@TESTSET_LM-9097\"",
    "trades": "npx cypress run -e TAGS=\"@TESTSET_LM-9075\"",
    "transfers": "npx cypress run -e TAGS=\"@TESTSET_LM-9075\"",
    "stg-monitoring": "npx cypress run --config baseUrl=https://app-staging.membranelabs.com -e TAGS=\"@monitoring\" --record --key",
    "prod-monitoring": "npx cypress run --config baseUrl=https://app.membranelabs.com -e TAGS=\"@monitoring\" --record --key",
    "prod-e2e": "npx cypress run --config baseUrl=https://app.membranelabs.com -e TAGS=\"@regression\" --record --key",
    "staging-e2e": "npx cypress run --config baseUrl=https://app-staging.membranelabs.com -e TAGS=\"@regression\" --record --key"

  },
  "author": "Xblend",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@currents/cli": "^3.0.0",
    "cypress": "^10.11.0",
    "multiple-cucumber-html-reporter": "^3.0.1",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^13.0.3",
    "@cypress/webpack-preprocessor": "^5.15.0",
    "cypress-dotenv": "^2.0.0",
    "cypress-xpath": "^2.0.1",
    "dotenv": "^16.0.3",
    "moment": "^2.29.4",
    "node-polyfill-webpack-plugin": "^2.0.1"
  }
}
